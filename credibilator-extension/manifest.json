  {
    "name": "Credibilator",
    "version": "1.0.1",
    "description": "Analyses pages to assess (and explain) their credibility. Uses AI to learn from previously seen trustworthy or fake news sources.",
    "permissions": ["https://cs.uns.edu.ar/","activeTab"],
    "background": {
      "page": "background.html",
      "persistent": false
    },
    "icons": {
    	"16": "img/icon16.png",
        "48": "img/icon48.png",
        "128": "img/icon128_pad.png"
    },
    "browser_action": {
      "default_popup": "popup.html"
    },
    "content_security_policy": "script-src 'self' https://cs.uns.edu.ar; object-src 'self'",
    "manifest_version": 2,
    "homepage_url": "https://github.com/piotrmp/credibilator"
    }
